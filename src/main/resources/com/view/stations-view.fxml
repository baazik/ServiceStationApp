<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox prefHeight="400.0" prefWidth="600.0" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mb.company.controllers.StationsController">
    <children>
        <TableView fx:id="stationsTableView">
            <columns>
                <TableColumn fx:id="idColumn" text="ID" />
                <TableColumn fx:id="nameColumn" text="Name" />
                <TableColumn fx:id="addressColumn" text="Address" />
                <TableColumn fx:id="countryColumn" text="Country" />
            </columns>
        </TableView>
        <Button fx:id="backToMenuButton" mnemonicParsing="false" onAction="#backToMainMenu" text="Back to Main Menu" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
